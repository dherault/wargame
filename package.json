{
  "name": "basicwars.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "./node_modules/.bin/mocha --require @babel/register",
    "eject": "react-scripts eject",
    "sync-s3": "aws s3 sync build/ s3://basicwars.com --region eu-west-3",
    "invalidate-cloudfront": "aws cloudfront create-invalidation --distribution-id E3JR8XO6CA1OSL --paths /*",
    "deploy": "npm run sync-s3 && npm run invalidate-cloudfront",
    "build-deploy": "npm run build && npm run deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "connected-react-router": "^6.3.1",
    "flexpad": "^0.2.0",
    "history": "^4.7.2",
    "normalize.css": "^8.0.1",
    "piano-keys": "^1.0.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^6.0.2"
  }
}
