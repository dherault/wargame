{
  "name": "basicwars.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "npm run bump-version && react-app-rewired build",
    "test": "./node_modules/.bin/mocha --require @babel/register",
    "eject": "react-scripts eject",
    "analyze": "npx source-map-explorer build/static/js/*.js",
    "lint": "./node_modules/.bin/eslint src test",
    "bump-version": "node ./scripts/bumpVersion.js",
    "sync-s3": "aws s3 sync build/ s3://basicwars.com --region eu-west-3",
    "invalidate-cloudfront": "aws cloudfront create-invalidation --distribution-id E3JR8XO6CA1OSL --paths /*",
    "deploy": "npm run sync-s3 && npm run invalidate-cloudfront",
    "build-deploy": "npm run build && npm run deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "clipboard-copy": "^3.0.0",
    "connected-react-router": "^6.4.0",
    "flexpad": "^0.2.0",
    "history": "^4.9.0",
    "piano-keys": "^1.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-dherault": "^1.0.0",
    "mocha": "^6.1.4",
    "react-app-rewired": "^2.1.3",
    "worker-loader": "^2.0.0"
  }
}
